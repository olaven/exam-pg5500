{
    "url": "https://api.entur.io/journey-planner/v2/graphql",
    "event": "transport_event",
    "json": {
        "query": "{trip(dateTime:\"{{arrive_by}}\",from: {place: \"NSR:StopPlace:{{{from_id}}}\"}, to: {place: \"NSR:StopPlace:{{{to_id}}}\"}, arriveBy:true ) {tripPatterns {startTime}}}"
    },
    "coreid": "{{{PARTICLE_DEVICE_ID}}}",
    "published_at": "{{{PARTICLE_PUBLISHED_AT}}}",
    "mydevices": true,
    "requestType": "POST",
    "responseTopic": "transport_response",
    "responseTemplate": "{{{data.trip.tripPatterns.0.startTime}}}"
}